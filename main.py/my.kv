WindowManager:
    LogIn:
    CreateAccount:
    WorkMode:

<TextInput>:
    size_hint: 0.3, 0.1

<Button>:
    font_size: 30
    color: 0.3,0.6,0.7,1
    size_hint: 0.3, 0.1
    background_color: 0.3,0.4,0.5,1

<LogIn>:
    name: "login"
    email: email
    password: password
    canvas.before:
        Color:
            rgba: 0.2, 0.75, 0.7, 0.7
        Rectangle:
            pos: self.pos
            size: self.size
    FloatLayout:
        size: root.width, root.height
        Button:
            text: "Log in"
            pos_hint: {"x": 0.2, "top": 0.3}
            on_release:
                root.LoginButton()
                root.manager.transition.direction = "up"

        Button:
            text: "New user"
            pos_hint: {"x":0.5, "top": 0.3}
            on_release:
                root.CreateButton()
                root.manager.transition.direction = "left"

        Label:
            font_size: 30
            color: 0,0,0,1
            size_hint: 0.3, 0.1
            text: "Enter Email:"
            pos_hint: {"x": 0.1, "top": 0.9}
    

        Label:
            font_size: 30
            color: 0,0,0,1
            size_hint: 0.3, 0.1
            text: "Enter Password:"
            pos_hint: {"x": 0.1, "top": 0.8}
     

        TextInput:
            id: email
            multiline: False
            pos_hint: {"x":0.5, "top": 0.9}

        TextInput:
            id: password
            multiline: False
            pos_hint: {"x": 0.5, "top": 0.8}

<CreateAccount>:
    name: "createaccount"
    namee: namee
    password: passw
    email: email

    canvas.before:
        Color:
            rgba: 0.2, 0.75, 0.7, 0.7
        Rectangle:
            pos: self.pos
            size: self.size
    FloatLayout:
        cols:1
        FloatLayout:
            Label:
                font_size: 30
                color: 0,0,0,1
                size_hint: 0.3, 0.1
                text: "Enter Email:"
                pos_hint: {"x": 0.1, "top": 0.9}


            Label:
                font_size: 30
                color: 0,0,0,1
                size_hint: 0.3, 0.1
                text: "Enter Password:"
                pos_hint: {"x": 0.1, "top": 0.8}
    

            Label:
                font_size: 30
                color: 0,0,0,1
                size_hint: 0.3, 0.1
                text: "Enter Name:"
                pos_hint: {"x": 0.1, "top": 0.7}


            TextInput:
                id: email
                multiline: False
                pos_hint: {"x":0.5, "top": 0.9}

            TextInput:
                id: passw
                multiline: False
                pos_hint: {"x": 0.5, "top": 0.8}
            
            TextInput:
                id: namee
                multiline: False
                pos_hint: {"x": 0.5, "top": 0.7}

        Button:
            pos_hint: {"x":0.2, "top": 0.3}
            text: "Submit"
            on_release:
                root.Submit()
                app.root.transition.direction = "right"

        Button:
            pos_hint: {"x":0.5, "top": 0.3}
            font_size: 20
            text: "Already have an account? Log In"
            on_release:
                root.login()
                app.root.transition.direction = "right"

<WorkMode>:
